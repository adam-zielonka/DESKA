{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "docker-compose",
      "args": ["up"],
      "problemMatcher": {
        "owner": "pascal",
        "fileLocation": ["relative", "${workspaceFolder}/src"],
        "pattern": [
          {
            "regexp": "(.*)\\((.*)\\): (([^ ]*) .*)",
            "file": 1,
            "line": 2,
            "message": 3,
            "severity": 4
          },
          {
            "regexp": "|\\s(.*)",
            "code": 1
          }
        ]
      }
    }
  ]
}
